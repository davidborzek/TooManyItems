{
  "name": "mobilecomputing",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "NODE_OPTIONS=--openssl-legacy-provider expo start --web",
    "format": "prettier --write src",
    "lint": "eslint --fix src",
    "test": "jest --silent",
    "test:updateSnapshots": "jest --silent -u",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.2",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7",
    "@supabase/supabase-js": "^2.2.0",
    "dotenv": "^16.0.3",
    "expo": "~47.0.8",
    "expo-camera": "~13.1.0",
    "expo-image-picker": "~14.0.2",
    "expo-print": "~12.0.0",
    "expo-sharing": "~11.0.1",
    "expo-status-bar": "~1.4.2",
    "i18next": "^22.4.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.5",
    "react-native-elements": "^3.4.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^29.2.5",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.6.3"
  },
  "private": true,
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --fix src",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  }
}
